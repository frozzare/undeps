#!/usr/bin/env node

const cwd = process.cwd();
const { used, unused } = require("../")(cwd);

console.log(
  `Found ${Object.keys(used).length} dependenc${
    Object.keys(used).length > 1 ? "ies" : "y"
  } and ${Object.keys(unused).length} are unused:`
);
console.log(`\n${Object.keys(unused).join("\n")}`);
