#!/usr/bin/env node

const cwd = process.cwd();
const { deps, diff } = require("../")(cwd);

console.log(
  `Found ${Object.keys(deps).length} dependenc${
    Object.keys(deps).length > 1 ? "ies" : "y"
  } and ${Object.keys(diff).length} are unused:`
);
console.log(`\n${Object.keys(diff).join("\n")}`);
